## production
id: number
title: string
status: number(0: incompletd, 1: completed)
created_at: DATETIME
updated_at: DATETIME

## db
CREATE TABLE todos (
  id INT AUTO_INCREMENT PRIMARY KEY,
  title VARCHAR(100) NOT NULL,
  status TINYINT NOT NULL DEFAULT 0,
  created_at DATETIME NOT NULL,
  updated_at DATETIME NOT NULL
);
